{
  "name": "bmkg-tools",
  "description": "BMKG Monorepo Tools",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "check-types": "turbo check-types",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules .turbo apps/*/dist packages/*/dist"
  },
  "devDependencies": {
    "turbo": "^2.6.3"
  },
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
